{
  "extends": "tslint:all",
  "linterOptions": {
    "exclude": [
      "**/*.js"
    ]
  },
  "rules": {
    "completed-docs": false,
    "file-name-casing": false,
    "member-access": false,
    "member-ordering": false,
    "no-any": false,
    "only-arrow-functions": false,
    "prefer-method-signature": false,
    "quotemark": [true, "single", "avoid-escape", "avoid-template"]
  }
}
